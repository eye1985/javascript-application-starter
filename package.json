{
  "name": "js-starter-kit",
  "version": "1.4.1",
  "description": "A starter pack for JS applications",
  "main": "app.js",
  "author": "Erik",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
