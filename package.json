{
  "name": "js-starter-kit",
  "version": "1.3.0",
  "description": "A starter pack for JS applications",
  "main": "app.js",
  "author": "Erik",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --hot --progress",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.3.0",
    "cssnano": "^5.0.8",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.2.4",
    "postcss-loader": "^6.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "url-loader": "^4.1.0",
    "webpack": "^5.20.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.3.0"
  }
}
